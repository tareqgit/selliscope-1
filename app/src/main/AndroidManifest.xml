            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyBZ5heET41mdUTW5UOouLZFn56kGsAocbM" />
        <meta-data
            android:name="com.google.android.awareness.API_KEY"
            android:value="AIzaSyBZ5heET41mdUTW5UOouLZFn56kGsAocbM" />-->
        <!--For debug APK-->

        <!--For signed APK-->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAhsz4pX62D7yu8twGT4ydn-_DRVB3p6_Y" />
        <meta-data
            android:name="com.google.android.awareness.API_KEY"
            android:value="AIzaSyAhsz4pX62D7yu8twGT4ydn-_DRVB3p6_Y" />
        <!--For signed APK-->

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="c2bca7824e6a758388318ecf6635fd2e8e11979a" />

        <!--Services and Receivers-->
        <service android:name="com.humaclab.selliscope_rangs.service.FcmInstanceIdServices">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name="com.humaclab.selliscope_rangs.service.FcmGetNotificationService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service
            android:name="com.humaclab.selliscope_rangs.service.SendLocationDataService"
            android:exported="false"
            android:permission="android.permission.RECEIVE_BOOT_COMPLETED"
            android:process=":selliscope_process">
            <intent-filter>
                <action android:name=".Service.SendLocationDataService" />
                <action android:name="android.content.SyncAdapter" />
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </service>
        <!--Services and Receivers-->

        <activity
            android:name="com.humaclab.selliscope_rangs.activity.SplashScreenActivity"
            android:theme="@style/SplashScreenTheme"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW"/>

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.humaclab.selliscope_rangs.activity.LoginActivity"
            android:noHistory="true" />
        <activity
            android:name="com.humaclab.selliscope_rangs.activity.HomeActivity"
            android:configChanges="orientation|screenSize" />
        <activity android:name="com.humaclab.selliscope_rangs.activity.RouteActivity" />
        <activity android:name="com.humaclab.selliscope_rangs.activity.OutletActivity" />
        <activity android:name="com.humaclab.selliscope_rangs.activity.AddOutletActivity" />
        <activity android:name="com.humaclab.selliscope_rangs.activity.OutletDetailsActivity" />
        <activity android:name="com.humaclab.selliscope_rangs.activity.PrivacyPolicyActivity" />
        <activity android:name="com.humaclab.selliscope_rangs.activity.ProductActivity" />
        <activity
            android:name="com.humaclab.selliscope_rangs.activity.OrderActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.humaclab.selliscope_rangs.activity.DeliveryListActivity" />
        <activity
            android:name="com.humaclab.selliscope_rangs.activity.DeliveryDetailsActivity"
            android:screenOrientation="landscape" />
        <activity android:name="com.humaclab.selliscope_rangs.activity.PaymentActivity" />
        <activity android:name="com.humaclab.selliscope_rangs.activity.DueActivity" />
        <activity android:name="com.humaclab.selliscope_rangs.activity.EditOutletActivity" />
        <activity android:name="com.humaclab.selliscope_rangs.activity.CallCardActivity" />
        <activity android:name="com.humaclab.selliscope_rangs.activity.InspectionActivity" />
        <activity android:name="com.humaclab.selliscope_rangs.activity.SalesReturnActivity" />
        <activity
            android:name="com.humaclab.selliscope_rangs.activity.SalesReturnDetailsActivity"
            android:screenOrientation="landscape" />
    </application>

</manifest>