        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="c2bca7824e6a758388318ecf6635fd2e8e11979a"/>

        <!--Services and Receivers-->
        <service
            android:name="com.humaclab.selliscope_myone.service.FcmInstanceIdServices">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
        <service
            android:name="com.humaclab.selliscope_myone.service.FcmGetNotificationService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
        <service
            android:name="com.humaclab.selliscope_myone.service.SendLocationDataService"
            android:exported="false"
            android:permission="android.permission.RECEIVE_BOOT_COMPLETED"
            android:process=":selliscope_process">
            <intent-filter>
                <action android:name=".Service.SendLocationDataService"/>
                <action android:name="android.content.SyncAdapter"/>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </service>
        <!--Services and Receivers-->

        <activity
            android:name="com.humaclab.selliscope_myone.activity.SplashScreenActivity"
            android:theme="@style/SplashScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.humaclab.selliscope_myone.activity.LoginActivity"/>
        <activity
            android:name="com.humaclab.selliscope_myone.activity.HomeActivity"
            android:configChanges="orientation|screenSize"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.RouteActivity"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.OutletActivity"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.AddOutletActivity"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.OutletDetailsActivity"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.PrivacyPolicyActivity"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.ProductActivity"/>
        <activity
            android:name="com.humaclab.selliscope_myone.activity.OrderActivity"
            android:screenOrientation="landscape"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.DeliveryListActivity"/>
        <activity
            android:name="com.humaclab.selliscope_myone.activity.DeliveryDetailsActivity"
            android:screenOrientation="landscape"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.PaymentActivity"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.DueActivity"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.EditOutletActivity"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.CallCardActivity"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.InspectionActivity"/>
        <activity android:name="com.humaclab.selliscope_myone.activity.SalesReturnActivity"/>
        <activity
            android:name="com.humaclab.selliscope_myone.activity.SalesReturnDetailsActivity"
            android:screenOrientation="landscape"/>
    </application>

</manifest>