  &lt;!&ndash;Services and Receivers&ndash;&gt;
        <service android:name="com.humaclab.selliscope_myone.service.FcmInstanceIdServices">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
